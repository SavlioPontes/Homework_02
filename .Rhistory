gerar_temp <- function(n){
n_pares <- n/2
u1 = runif(n_pares)
u2 = runif(n_pares)
z1 = sqrt(-2 * log(u1)) * cos(2 * pi * u2)
z2 = sqrt(-2 * log(u1)) * sin(2 * pi * u2)
z <- c(z1,z2)
temp <- 62 + 3.5*z
return(temp)
}
dados_cpu <- gerar_temp(1000)
head(dados_cpu)
